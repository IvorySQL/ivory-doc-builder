{
  "name": "ivory-doc-builder-withantora",
  "version": "1.0.0",
  "scripts": {
    "build:en": "npx antora generate --stacktrace --to-dir ../www_publish_target/docs/en antora-playbook-EN.yml",
    "build:cn": "npx antora generate --stacktrace --to-dir ../www_publish_target/docs/cn antora-playbook-CN.yml",
    "build:all": "npm run build:en && npm run build:cn"
  },
  "keywords": [
    "ivorysql",
    "antora",
    "documentation",
    "multi-language"
  ],
  "dependencies": {
    "@antora/lunr-extension": "^1.0.0-alpha.11",
    "@antora/pdf-extension": "^1.0.0-alpha.20",
    "@node-rs/jieba": "^1.10.3",
    "antora": "3.1.7"
  }
}